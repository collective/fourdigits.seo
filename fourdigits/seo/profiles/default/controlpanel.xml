<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="fourdigits.seo">

  <configlet
      title="SEO"
      action_id="fourdigits.seo.settings"
      appId="fourdigits.seo"
